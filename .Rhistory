install.packages(c("tidyverse", "ggpubr"))
library(tidyverse)
library(ggpubr)
# carregar banco de dados
data <- read_csv("data/insurance.csv")
# ver estrutura do banco de dados
glimpse(data)
data %>%
geom(aes(x=age, y=sex)) +
geom_histogram()
data %>%
ggplot(aes(x=age, y=sex)) +
geom_histogram()
data %>%
ggplot(aes(x=age, fill=sex)) +
geom_histogram()
data %>%
ggplot(aes(x=age, fill=sex)) +
geom_density()
data %>%
ggplot(aes(x=age, fill=sex)) +
geom_boxplot()
data %>%
ggplot(aes(x = age^2, y = charges)) +
geom_point()
data %>%
ggplot(aes(x = age, y = charges)) +
geom_point()
data %>%
ggplot(aes(x = age, y = log(charges+1))) +
geom_point()
data %>%
ggplot(aes(x = age, y = charges)) +
geom_point()
data %>%
ggplot(aes(x = age, y = charges, colour = smoker)) +
geom_point()
data %>%
ggplot(aes(x = age, y = charges, colour = smoker)) +
geom_point() +
geom_facet( ~ bmi)
data %>%
ggplot(aes(x = age, y = charges, colour = smoker)) +
geom_point() +
facet_wrap( ~ bmi)
data %>%
ggplot(aes(x = age, y = charges, colour = smoker)) +
geom_point() +
facet_grid( ~ bmi)
data %>%
ggplot(aes(x = age, y = charges)) +
geom_point() +
facet_grid( ~ bmi)
